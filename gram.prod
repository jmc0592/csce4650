%start statement_list

%%
statement_list  :   statement_list statement
                |   statement
                ;

statement   :   ID "=" INTLITERAL ';'
        {
            $$ = $1->s;//change to our record
            generate_copy($1, $3);
        }
        |
        {
            $$ = $1->s;//change to our record
            generate_exp($1, $3);
        }
    ;

